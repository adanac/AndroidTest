<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.adanac.test"
    android:versionCode="1002"
    android:versionName="2.2" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="19" />
    
    <!-- 添加 网络权限 -->
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.adanac.test.view.WebViewActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity 
            android:name="com.adanac.test.MainActivity"
            android:label="@string/app_name"></activity>
        
        <activity
            android:name="com.adanac.test.layout.LayoutActivity"
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name="com.adanac.test.event.ImageBtnActivity"
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name="com.adanac.test.event.RadioBtnActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.adanac.test.event.ProgressBarActivity"
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name="com.adanac.test.event.TabHostActivity"
            android:label="@string/app_name" >
        </activity>
        <activity 
            android:name="com.adanac.test.event.ListViewActivity"
            android:label="@string/app_name"></activity>
         <activity 
            android:name="com.adanac.test.event.FirstBtnActivity"
            android:label="@string/app_name"></activity>
         <activity 
            android:name="com.adanac.test.event.SecondBtnActivity"
            android:label="@string/app_name"></activity>
        
    </application>

</manifest>
